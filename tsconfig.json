{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./lib",
    "allowJs": true,
    "target": "ES2022",
    "moduleResolution":"node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["./src/**/*"],
}
